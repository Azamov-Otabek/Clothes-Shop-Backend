{
    "name": "my-project",
    "version": 2,
    "builds": [
      { "src": "src/index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/", "dest": "/index.js" }
    ],
    "env": {
      "API_URL": "https://example.com/api"
    },
    "public": true,
    "regions": ["us-east-1", "us-west-1"]
  }
  